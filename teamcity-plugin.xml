<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>docker-deploy</name>
        <display-name>Docker Deploy</display-name>
        <version>@Version@</version>
        <description>Allows building and deploying projects into Docker containers</description>
        <vendor>
            <name>Codeamatic</name>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" />
</teamcity-plugin>
